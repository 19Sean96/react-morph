{
  "name": "react-morph",
  "version": "0.0.1",
  "description": "Ui transitions made simple",
  "main": "lib/index.js",
  "scripts": {
    "preversion": "npm run test && npm run build",
    "prepublishOnly": "npm run build",
    "build": "rm -rf lib/* && babel src --out-dir lib && babel src --out-file lib/all.js && uglifyjs lib/all.js -m -c -o lib/all.min.js && rm -f lib/all.js",
    "dev": "rm -rf lib/* && babel --watch src --out-dir lib --source-maps",
    "test": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunnolou/react-morph.git"
  },
  "contributors": [
    "Bruno Louren√ßo"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brunnolou/react-morph/issues"
  },
  "homepage": "https://github.com/brunnolou/react-morph#readme",
  "dependencies": {
    "eslint-plugin-flowtype": "^2.44.0",
    "keyframe": "^2.1.0",
    "popmotion": "^8.1.10",
    "prop-types": "^15.6.0",
    "react": "^16.2.0"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-react-app": "^3.1.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-config-skeleton": "^0.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "prettier": "^1.9.2",
    "prettier-eslint": "^8.7.0",
    "uglify-js": "^3.2.2"
  }
}
